
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table style="border: 2px solid black">
            @for(int i = 0; i < 12; i++)
            {
                <tr>

                @for (int j = 0; j < 10; j++)
                {
                    <td style="border: 2px solid black">
                        @(i+1) * @(j+1) = @((i + 1) * (j + 1))
                    </td>
                }

                </tr>
            }
        </table>
    </div>
</body>
</html>
